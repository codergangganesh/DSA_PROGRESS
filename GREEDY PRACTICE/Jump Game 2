class Solution {
    public int jump(int[] nums) {
        int max_i = 0,cur_e = 0,j = 0;
        for (int i =0;i<nums.length-1;i++){
            if (i+nums[i]>max_i)
                max_i = i+nums[i];
            if (i==cur_e){
                cur_e=max_i;
                j++;
            }
        }
        return j;
    }
}